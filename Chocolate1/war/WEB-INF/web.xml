<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Chocolate1</servlet-name>
		<servlet-class>ac.tsuda.Chocolate1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Chocolate1</servlet-name>
		<url-pattern>/chocolate1</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GoogleLogin</servlet-name>
		<servlet-class>ac.tsuda.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>add_linkdata</servlet-name>
		<servlet-class>ac.tsuda.AddLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add_linkdata</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>del_linkdata</servlet-name>
		<servlet-class>ac.tsuda.DelLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>del_linkdata</servlet-name>
		<url-pattern>/del</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>login.html</welcome-file>
	</welcome-file-list>
</web-app>
